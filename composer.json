{
    "name": "pinfirelabs/pcm-integrations",
    "type": "craft-plugin",
    "version": "1.0.2",
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "threelakessoftware/shared-events-calendar": "dev-craft-integration"
    },
    "autoload": {
        "psr-4": {
          "pinfirelabs\\pcmIntegrations\\": "src/"
        }
    },
    "extra": {
        "name": "PCM Integrations",
        "handle": "pcm-integrations",
        "hasCpSettings": false,
        "hasCpSection": false,
        "class": "pinfirelabs\\pcmIntegrations\\Plugin"
    },
    "repositories": [
        {
			"type": "vcs",
			"url": "git@bitbucket.org:threelakessoftware/shared-events-calendar.git"
		}
    ]
}
